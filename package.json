{
	"name": "@agnosticeng/heroicons-svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "eslint . --fix",
		"generate:icons": "node ./scripts/generate-icons.mjs",
		"cleanup": "rm -rf ./dist && find ./src/lib -name \"*.svelte\" -type f -exec rm -rf {} +",
		"generate": "npm run generate:icons && npm run format",
		"update_version": "node ./scripts/update-package.mjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/agnosticeng/heroicons-svelte.git"
	},
	"author": "Yann Amsellem",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/agnosticeng/heroicons-svelte/issues"
	},
	"homepage": "https://github.com/agnosticeng/heroicons-svelte#readme",
	"exports": {
		".": {
			"types": "./dist/index.d.ts"
		},
		"./outline/*": {
			"types": "./dist/outline/*.svelte.d.ts",
			"svelte": "./dist/outline/*.svelte"
		},
		"./solid/*": {
			"types": "./dist/solid/*.svelte.d.ts",
			"svelte": "./dist/solid/*.svelte"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/package": "^2.3.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-svelte": "^2.44.0",
		"globals": "^15.9.0",
		"heroicons": "^2.1.5",
		"minimist": "^1.2.8",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.11",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.2",
		"svgson": "^5.3.1",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.6.0",
		"vite": "^5.4.6"
	}
}
