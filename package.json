{
	"name": "@agnostic/heroicons-svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package --input src/lib && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "eslint . --fix",
		"generate:icons": "node ./scripts/generate-icons.mjs",
		"cleanup:icons": "find ./src/lib -name \"*.svelte\" -type f -exec rm -rf {} +",
		"generate": "npm run cleanup:icons && npm run generate:icons && npm run format"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts"
		},
		"./outline/*": {
			"types": "./dist/outline/*.svelte.d.ts",
			"svelte": "./dist/outline/*.svelte"
		},
		"./solid/*": {
			"types": "./dist/solid/*.svelte.d.ts",
			"svelte": "./dist/solid/*.svelte"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/package": "^2.3.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-svelte": "^2.44.0",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.11",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.2",
		"svgson": "^5.3.1",
		"typescript": "^5.6.2",
		"vite": "^5.4.6"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"heroicons": "^2.1.5"
	}
}
